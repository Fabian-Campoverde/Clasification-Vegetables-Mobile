
{
  "version": 2,
  "engines": {
    "node": "18.x"
  },
  "builds": [
    {
      "src": "image_classifier/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
  
    {
      "src": "/(.*)",
      "dest": "image_classifier/wsgi.py"
    },
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
  ]
}

