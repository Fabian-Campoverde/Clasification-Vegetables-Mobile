
{
  "version": 2,
  "builds": [
    {
      "src": "image_classifier/wsgi.py",
      "use": "@vercel/python"
     
    },
    {
        "src": "build-files.sh",
        "use": "@vercel/build-utils"
      }
  ],
  "routes": [
  
    {
      "src": "/(.*)",
      "dest": "image_classifier/wsgi.py"
    },
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
  ]
}

